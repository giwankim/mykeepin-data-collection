html, body, .wrapper {width:100%; height:100%;}
.main_wrap {width:100%; height:100%; min-height:900px; display:flex; }
.main_img { width:calc(50% + 17px); background:url(../images/main/main_img.jpg) no-repeat right center; background-size:cover;  transition:background ease 0.3s;}
.main_img h1 {font-size:0; line-height:0; text-indent:-9999999999px;}
.main_con {width:calc(50% - 17px); display:flex; justify-content:center; align-items:center; flex-wrap:wrap; position:relative;}
.main_con .con {max-width:486px; padding-bottom:200px;}
.main_con .con h2 {font-size:45px; margin-bottom:10px; color:#333;}
.main_con .con p {font-size:18px; color:#888888;}
.main_con .con ul {display:flex; justify-content:space-between; margin-top:24px; margin-bottom:35px;}
.main_con .con ul li {width:calc(50% - 5px);}
.main_con .con ul li a {display:flex; justify-content:center; align-items:center; height:150px; background:#4d544a; border-radius:5px; transition:background ease 0.3s;}
.main_con .con ul li a:hover {background:#000;}
.main_con .con ul li.orange a {background:#e74d2e;}
.main_con .con ul li.orange a:hover {background:#f73008;}
.main_con .con ul li a span {color:#fff; font-size:22px; font-weight:500; padding-top:50px;}
.main_con .con ul li a span.ico01 {background:url(../images/main/main_ico01.png) no-repeat center 0;}
.main_con .con ul li a span.ico02 {background:url(../images/main/main_ico02.png) no-repeat center 0;}
.main_con .con_footer p {font-size:16px; color:#777777;}
.main_con .con_footer a {display:inline-block; font-size:16px; color:#666; padding-right:25px; background:url(../images/main/main_arrow.png) no-repeat; background-position:right 64%; font-weight:500; transition:color ease 0.3s;}
.main_con .con_footer a:hover {color:#e74d2e;}

.main_footer {max-width:486px; position:absolute; bottom:80px; left:50%; margin-left:-243px;}
.main_footer .f_menu {display:flex; }
.main_footer .f_menu li {margin-right:24px;}
.main_footer .f_menu li a {font-size:16px; color:#333; font-weight:500;}

.main_footer .f_add {display:flex; flex-wrap:wrap; margin-top:15px;}
.main_footer .f_add li {font-size:15px; color:#555; position:relative; padding:0 12px; line-height:1.6;}
.main_footer .f_add li::before {position:absolute; top:50%; margin-top:-5px; left:0; width:1px; height:12px; background:#bbb; content:'';}
.main_footer .f_add li:first-child,.main_footer .f_add li:last-child,.main_footer .f_add li:nth-child(2) 
{padding-left:0;}
.main_footer .f_add li:first-child::before,.main_footer .f_add li:last-child::before,.main_footer .f_add li:nth-child(2)::before
{display:none;}

.main_footer .f_add li:last-child {margin-top:10px;}





.sub_wrap {width:100%; height:100%; /*min-height:900px; display:flex;*/ }
.sub_img { width:580px; background:url(../images/content/con_bg.jpg) no-repeat right center; background-size:cover;  transition:background ease 0.3s;position:relative;}
.sub_img > div {width:304px; margin:0 auto; margin-top:200px;}
.sub_img > div .num {position:relative; display:flex;}
.sub_img > div .num::before {position:absolute; top:50%; left:6px; width:45px; height:1px; background:#c7c5c1; transform:rotate(-60deg); content:'';}
.sub_img > div .num strong {font-size:30px; color:#e74d2e; font-weight:500; line-height:1.3;}
.sub_img > div .num span {font-size:30px; color:#777; font-weight:300; margin-top:10px; margin-left:20px;}

.sub_img > div h2 {font-size:45px; color:#333; line-height:1.3; margin-top:15px;}

.sub_con {float:right; height:100%;width:calc(100% - 580px); position:relative; display:flex; justify-content:center; align-items:center; flex-wrap:wrap;}

.scon01 {width:500px; padding-bottom:100px;}
.sub_footer {min-width:100%; padding-bottom:50px; position:relative; width:100%; bottom:auto; left:auto; margin:0;}
.main_footer.sub_footer .f_menu {justify-content:center;}
.sub_footer.main_footer .f_add li:nth-child(2)::before {display:block;}
.sub_footer.main_footer .f_add {justify-content:center;}
.sub_footer.main_footer .f_add li {padding:0 8px;}
.sub_footer.main_footer .f_add li:nth-child(2) {padding-left:8px;}
.sub_footer.main_footer .f_add li:last-child {margin-top:0px; width:100%; text-align:center;}

.con_bt_back {font-size:0; text-indent:-999999999px; display:block; width:26px; height:21px; background:url(../images/content/scon_arrow.png) no-repeat 0 0; margin-bottom:40px;}
.con_bt_back:hover {font-size:0;}

.con_form > div {width:100%; margin-top:25px;}
.con_form > div:first-child {margin-top:0;}
.con_form > div h3 {width:100%; font-size:18px; color:#222;}
.con_form > div h3 span {display:inline-block; margin-left:10px; font-size:15px; color:#777;}

.con_form_input {width:100%; border-bottom:1px solid #ccc; margin-top:5px;}
.con_form_input input {border:0; padding:14px 10px; box-sizing:Border-box; width:100%; font-size:17px; color:#333; font-weight:400;}
.con_form_input input::placeholder {font-weight:300;  }

.con_form_select {padding:14px 10px;box-sizing:Border-box; width:100%; border:0; font-size:17px; color:#333; 
 -webkit-appearance: none; -moz-appearance: none; appearance: none; background:url(../images/content/select_arrow.png) no-repeat right 15px center; 
}


.con_form_select::-ms-expand { display:none; }

.con_form_input02 input {width:calc(100% - 30px);}
.con_form_input02 span {width:30px; font-size:15px; color:#444; font-weight:500; display:inline-block;}

.con_form_input03 {display:flex;}
.con_form_input03 input {width:calc(100% - 100px);}
.con_form_input03 a {width:100px; height:40px; border-radius:5px; background:#333; color:#fff; display:flex; align-items:center; justify-content:center; font-size:17px; margin-top:5px;}

.con_form_bt {display:flex; margin-top:40px; width:100%; height:70px; background:#e74d2e; color:#fff; justify-content:center; align-items:center; font-size:18px; border-radius:5px; transition:background ease 0.3s;}
.con_form_bt:hover {color:#fff; background:#e52e27;}

.con_form_input04 {display:flex; justify-content:space-between; align-items:center;}
.con_form_input04 input {width:100%;}
.con_form_input04 span {width:60px; font-size:15px; color:#444; font-weight:500; display:inline-block;}

.con_form_radio { margin-bottom:35px;}
.con_form_radio ul {display:flex; margin-top:10px;}
.con_form_radio ul li:not(:first-child) {margin-left:26px;}

.con_form_radio ul li label,
.con_form_radio ul li input[type="radio"] + span,
.con_form_radio ul li input[type="radio"] + span::before
{
    display: inline-block;
    vertical-align: middle;
}
 
.con_form_radio ul li label *
{
    cursor: pointer;
}
 
.con_form_radio ul li input[type="radio"]
{
    opacity: 0;
    position: absolute;
}
 
.con_form_radio ul li input[type="radio"] + span
{
   font-size:17px;
   padding-left:25px;	
   position:relative;
   font-weight:500;
   color:#333;
}
 

 

 
.con_form_radio ul li input[type="radio"] + span::before
{
   content:'';
	position:absolute;
	top:50%; left:0;
	background:url(../images/content/radio_uncheck.png) no-repeat 0 0;
	width:18px; height:18px;
	margin-top:-8px;
}
 
.con_form_radio ul li input[type="radio"]:checked + span::before
{
    color: #333;
}
 
.con_form_radio ul li input[type="radio"]:disabled + span
{
    cursor: default;
     
    -moz-opacity: .4;
    -webkit-opacity: .4;
    opacity: .4;
}

 
.con_form_radio ul li input[type="radio"]:checked + span::before
{
	background:url(../images/content/radio_check.png) no-repeat 0 0;
	
}




.scon02 {max-width:1120px; width:100%; padding:0 60px; box-sizing:Border-box; margin-top:100px;}
.scon02 .con_form_bt {max-width:500px; margin-left:auto; margin-right:auto;}

